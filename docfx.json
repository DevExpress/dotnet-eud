{
  "build": {
    "content": [
      {
        "files": [
            "eud-for-bi-dashboards/**/*.{md,yml}",
            "eud-for-devexpress-reports/**/*.{md,yml}",
            "toc.yml",
            "index.md"
          ],
        "exclude": [
          "_*/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
            "favicon.ico",
            "logo.svg",
            "eud-for-bi-dashboards/images/**",
            "eud-for-devexpress-reports/images/**"
          ],
          "exclude": [
            "_*/**"
          ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "./.template"
    ],
    "sitemap": {
      "baseUrl": "https://devexpress.github.io/"
    },
    "globalMetadata": {
      "_appName": "DevExpress EUD",
      "_appTitle": "DevExpress End-User Documentation",
      "_appFaviconPath": "favicon.ico",
      "_appFooter": "Copyright (c) 1998-2025 Developer Express Inc. All rights reserved.",
      "_appLogoPath": "logo.svg",
      "_enableSearch": true
    }
  }
}
